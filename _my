<script type="text/javascript">
(function() {
    var func = function() {
        var dialogQueue = [];
        var endFuncs = [];

        $.pageData.eventHandler = {
            listener: null,

            run: function(callback) {
                if (callback) {
                    dialogQueue.push(callback);
                } else {
                    dialogQueue.push($.pageData.eventHandler.end);
                }
                $.appUtil.callNestedFunctions(dialogQueue);
            },

            end: function() {
                var i;
                for (i = 0; i < endFuncs.length; i++) {
                    var func = endFuncs[i];
                    func();
                }
            },

            getEvents: function() {
                var events = [  ];

                return events;
            },

            setListener: function(listener) {
                this.listener = listener;
            },

            getListener: function() {
                return this.listener;
            }
        };

    };

    $.hjax.addLoadedFunc(func);
})();
</script>

