$.animation.manager.setupControllers();

$.hjax.addLoadedFunc(function() {
    // 念の為にホームに行ったら、リセットしておく
    $.hjax.resetBackBtnCover();

    $.pageData.my = {
        mission: {
            missionId: parseInt("0"),
            isEventMission: parseInt("0"),
            isOpen: parseInt("0"),
        },
        event: {
            eventId: parseInt("0"),
            eventType: parseInt("0"),
            canEvent: parseInt("0"),
        },
        showMenu: function() {
            $.audio.playSe('ok');
            return $.dialogUtil.showMenuDialog({ title: "More" });
        },
        onGo: function() {
            $.audio.playSe("ok");
            $.appUtil.onGoJourney(this.mission, this.event);
        },
        fbConnect: {
            isCheckFbConnectStatusEnable: false
        },
        onEventIntroduction: function() {
            $.hjax({se:'ok', url: '_event_introduction'});
        }
    };

    var queue = [];
    var reloadFlg = false;
    var loginRewardFlg = false;
    var banunuFlg = false;

    

    

    

    

    if (loginRewardFlg) {
        queue.push(function(done) {
            $.pageData.eventHandler.run(done);
        });
    }

    

    

    if (banunuFlg) {
        queue.push(function() {
            $.hjax({url: '_banunu_cm'});
        });
    } else if (reloadFlg) {
        queue.push(function() {
            $.hjax.reload();
        });
    }

    $.appUtil.callNestedFunctions(queue);
});

// update time
$.hjax.addLoadedFunc(function() {
    var updateTime = function() {
        $.appUtil.updateMpTime(100, 100, 1431553187);
        $.appUtil.updateBpTime(3, 3, 1431551733);


    };
    updateTime();

    $.pageData.updateTimerId = setInterval(updateTime, 1000);
    if ($.pageData.my.fbConnect.isCheckFbConnectStatusEnable) {
        $.appAjax({
            url: '_check_fb_connect_status',
            successFunc: function(data, dataType) {
                console.log(JSON.stringify(data));
                $.pageData.my.fbConnect.isCheckFbConnectStatusEnable = false;
            },
        });
    }

});

$.hjax.addUnloadedFunc(function() {
    clearInterval($.pageData.updateTimerId);
});

// rotating banner
$.hjax.addLoadedFunc(function() {
    var updateCampaignTime = function() {
        var overText = '';
    };
    //updateCampaignTime();

    //$.pageData.updateCampaignTimeID = setInterval(updateCampaignTime, 1000);

    var specialCampaign = [];
    var banners = [];

    var bannerLength = banners.length;
    if(bannerLength >= 3) {
        var lastBanner = {
            end_date: banners[bannerLength-1].end_time,
            home_banner: banners[bannerLength-1].src,
            common_url: banners[bannerLength-1].url,
        };

        specialCampaign.unshift(lastBanner);
    }

    var numberOfBanner = specialCampaign.length;
    var index = 0;
    var selectorID = '';


    var rotateBanner = function() {
        if (index == 0) {
            selectorID = "#banner-" + bannerLength;
        } else {
            selectorID = "#special-campaign-" + index;
        }
        for (var i = 1; i <= numberOfBanner; i++) {
            $("#special-campaign-" + i).addClass('hjax-none');
        }
        $("#banner-" + bannerLength).addClass('hjax-none');
        $(selectorID).removeClass('hjax-none');
        ++index;
        if (index == numberOfBanner) {
            index = 0;
        }
    };


    if ((numberOfBanner > 1) && (bannerLength >= 3)) {
        rotateBanner();
        $.pageData.rotateBannerID = setInterval(rotateBanner, 4000);
    }

});

$.hjax.addUnloadedFunc(function() {
        clearInterval($.pageData.rotateBannerID);
        //clearInterval($.pageData.updateCampaignTimeID);
});

</script>

</body>
</html>
